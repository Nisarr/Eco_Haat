{
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "**/*.{html,css,js,png,jpg,jpeg,gif,ico,svg}",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}